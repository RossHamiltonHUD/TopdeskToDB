<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
        
    <!--You can generate an Application Password through the "My Settings" page on Topdesk,
        through the 'user menu' in the top right of the dashboard-->
    <!--Please note, your account needs to be in the API permission group-->
    <add key="config_topdesk_email_address" value="" />
    <add key="config_topdesk_application_password" value="" />

    <!--The below sets the number of tickets to return per API call. (Max of 10k usually fastest) -->
    <add key="config_topdesk_api_page_size" value="10000"/>

    <!--The year you want your data to start at (2016 to 2024 supported with this version)-->
    <add key ="config_starting_year" value="22"/>
  </appSettings>
</configuration>